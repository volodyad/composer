{
  "engines": {
    "@sp-temp/composer": "<%= @sp-temp/composerversion %>"
  },
  "name": "<%= appname %>",
  "version": "0.0.1",
  "description": "<%= appdescription %>",
  "scripts": {
    "prepublish": "mkdirp ./dist && @sp-temp/composer archive create --sourceType dir --sourceName . -a ./dist/<%= appname %>.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
  },
  "keywords": [
    "@sp-temp/composer",
    "@sp-temp/composer-network"
  ],
  "author": "<%= appauthor %>",
  "email": "<%= appemail %>",
  "license": "<%= applicense %>",
  "devDependencies": {
    "@sp-temp/composer-admin": "<%= @sp-temp/composerversion %>",
    "@sp-temp/composer-cli": "<%= @sp-temp/composerversion %>",
    "@sp-temp/composer-client": "<%= @sp-temp/composerversion %>",
    "@sp-temp/composer-common": "<%= @sp-temp/composerversion %>",
    "@sp-temp/composer-connector-embedded": "<%= @sp-temp/composerversion %>",
    "@sp-temp/composer-cucumber-steps": "<%= @sp-temp/composerversion %>",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  }
}
